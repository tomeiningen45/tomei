{
  "name": "Craigslist Hide",
  "version": "1.1",
  "description": "Hide Ads that you won't want to see anymore.",
  "permissions":  [
    "tabs",
    "http://*.craigslist.org/*",
    "https://*.craigslist.org/*"
  ],
 "content_scripts": [
    {
      "matches": ["http://*.craigslist.org/*"],
      "js": ["contents.js"]
    }
  ],
  "manifest_version": 2
}
